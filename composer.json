{
  "name": "jooplaan/govuk-blogs/",
  "description": "A WordPress theme based on gov.uk. ",
  "keywords": [
    "wordpress",
    "theme"
  ],
  "homepage": "https://github.com/jooplaan/govuk-blogs",
  "authors": [
    {
      "name": "Joop Laan",
      "homepage": "https://github.com/jooplaan/"
    }
  ],
  "type": "wordpress-theme",
  "require": {
    "php": "^7.2",
    "dxw/roots-composer": "dev-master#2fad80195fc15bd48329f398d256fcfd013ca0d5",
    "mustache/mustache": "^2.13",
    "dxw/php-missing": "^1.0",
    "dxw/result": "^1.0",
    "dxw/iguana": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "GovUKBlogs\\": "app/"
    }
  },
  "scripts": {
    "post-update-cmd": "vendor/bin/phar-install"
  },
  "require-dev": {
    "dxw/phar-install": "^1.1",
    "peridot-php/peridot": "^1.19",
    "peridot-php/leo": "^1.6",
    "peridot-php/peridot-dot-reporter": "^1.0",
    "php-mock/php-mock-mockery": "^1.3",
    "vimeo/psalm": "^3.12",
    "dxw/php-cs-fixer-config": "^1.0"
  }
}
